# Build output and dependencies
/target/                             # Maven build output
/target/*                             # Maven build output
!.mvn/wrapper/maven-wrapper.jar       # Keep maven wrapper
/node_modules                         # Node.js dependencies


# IntelliJ IDEA specific
.idea
*.iws
*.iml
*.ipr

# IDE and editor settings
/.idea                                # JetBrains settings
.project                              # Eclipse project settings
.classpath                            # Eclipse classpath settings
.settings/                            # Eclipse settings folder
*.launch                              # IDE launch configurations
.c9/                                  # Cloud9 IDE settings

# Visual Studio Code specific
.vscode/*
!.vscode/settings.json                # Keep important VSCode settings
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

# Temporary files and compiled output
/tmp                                  # Temporary files
/out-tsc                              # TypeScript compiler output
/dist                                 # Distribution files for builds

# Logs and lock files
npm-debug.log                         # NPM debug log
testem.log                            # Test runner log
/libpeerconnection.log                # WebRTC connection log
/connect.lock                         # Connect lock

# Miscellaneous
/.sass-cache                          # SASS cache
/coverage                             # Code coverage reports
/typings                              # TypeScript type definitions

# Specific project folders
/tecdoc                               # Ignoring TecDoc generated files
static/                               # Static assets to be ignored
src/main/java/com/automaterijal/application/tecdoc/  # Ignoring generated classes from TecDoc
src/main/java/META-INF/sun-jaxb.episode

# E2E tests
/e2e/*.js                             # End-to-end test JavaScript files

# Additional ignore rules for specific tools
/src/main/java/com/automaterijal/application/tecdoc/*.java
/target/*
